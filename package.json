{
  "name": "nextcut-backend",
  "version": "1.0.0",
  "description": "NextCut Backend API",
  "main": "dist/index.js",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "postinstall": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push"
  },
  "keywords": [
    "nextcut",
    "barber",
    "queue",
    "api"
  ],
  "author": "NextCut Team",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "prisma": "^5.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
